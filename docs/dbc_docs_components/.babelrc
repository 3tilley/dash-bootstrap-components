{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "env": {
        "production": {
            "plugins": [
                "@babel/plugin-proposal-object-rest-spread",
                "styled-jsx/babel"
            ]
        },
        "development": {
            "plugins": [
                "@babel/plugin-proposal-object-rest-spread",
                "styled-jsx/babel"
            ]
        },
        "test": {
            "plugins": [
                "@babel/plugin-proposal-object-rest-spread",
                "styled-jsx/babel-test"
            ]
        }
    },
    "plugins": [
        [
            "prismjs",
            {
                "languages": [
                    "javascript",
                    "css",
                    "html",
                    "python"
                ],
                "plugins": [
                    "line-numbers",
                    "show-language"
                ],
                "theme": "okaidia",
                "css": true
            }
        ]
    ]
}
